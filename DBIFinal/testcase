CREATE TABLE MYTABLE2 (att1 INTEGER, att2 DOUBLE, att3 STRING) AS SORTED ON att1, att2;
CREATE TABLE MYTABLE (att1 INTEGER, att2 DOUBLE, att3 STRING) AS HEAP;
DROP TABLE MYTABLE;
CREATE TABLE reg (r_regionkey INTEGER ,r_name STRING, r_comment STRING) AS SORTED ON r_name;
INSERT 'region.tbl' INTO reg;